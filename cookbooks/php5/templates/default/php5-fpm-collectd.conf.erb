<Plugin "curl_json">
  <URL "http://<%= node[:hostname] %>/fpm_status?json">
    Instance "php5-fpm"
    <Key "accepted conn">
      Type "counter"
    </Key>
    <Key "idle processes">
      Type "gauge"
    </Key>
    <Key "active processes">
      Type "gauge"
    </Key>
    <Key "total processes">
      Type "gauge"
    </Key>
  </URL>
</Plugin>
